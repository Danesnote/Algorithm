        "n = 1260\n",
        "count = 0\n",
        "\n",
        "#큰 단위의 화폐부터 차례로 확인\n",
        "coin_types = [500, 100, 50, 10]\n",
        "\n",
        "for coin in coin_types:\n",
        "  count += n // coin #동전 개수 세기\n",
        "  n %= coin # // 는 몫 % 는 나머지\n",
        "\n",
        "print(count)"
